<%@ Page Language="C#" AutoEventWireup="true" CodeFile="SingleTableQuery.aspx.cs" Inherits="SingleTable_SingleTableQuery" %>


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head id="Head1" runat="server">
    <title>Query</title>
</head>
<body>

    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <div class="box-name">
                        <%=strQueryTitle %>
                    </div>
                    <div class="box-icons">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="expand-link">
                            <i class="fa fa-expand"></i>
                        </a>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                    <div class="no-move"></div>
                </div>

                <div class="box-content">
                    <!-- BEGIN FORM-->
                    <form class="form-horizontal form-bordered form-row-strippedl" id="formQuery" runat="server">
                        <%=strQueryButton %>
                        <%=strQueryBody %>
                    </form>
                    <!-- END FORM-->
                </div>
            </div>

        </div>
    </div>
</body>
</html>
